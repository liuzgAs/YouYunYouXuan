<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <attr name="civ_fill_color" format="color" />
    </declare-styleable>
    <attr name="textwidth" format="integer"/>
    <attr name="typeface">
        <enum name="normal" value="0"/>
        <enum name="sans" value="1"/>
        <enum name="serif" value="2"/>
        <enum name="monospace" value="3"/>
    </attr>
    <declare-styleable name="CYTextView">
        <attr name="textwidth" />
        <attr name="textSize" format="dimension"/>
        <attr name="textColor" format="reference|color"/>
        <attr name="lineSpacingExtra" format="dimension"/>
        <attr name="typeface" />
    </declare-styleable>

    <declare-styleable name="ColorTrackTabViewIndicator">
        <attr name="ctTabSelectedTextColor" format="color"/>
        <attr name="ctTabIndicatorColor" format="color"/>
        <attr name="ctTabTextColor" format="color"/>
        <attr name="ctTabTextSize" format="dimension"/>
        <attr name="ctTabWidth" format="dimension"/>
        <attr name="ctTabGravity" format="enum">
            <enum name="Grivity_Fill" value="0"/>
            <enum name="Grivity_Center" value="1"/>
        </attr>
        <attr name="ctMode">
            <enum name="scrollable" value="0"/>
            <enum name="fixed" value="1"/>
        </attr>

    </declare-styleable>

    <declare-styleable name="ColorTrackView">
        <attr name="ctvText" format="string"/>
        <attr name="ctvText_size" format="dimension"/>
        <attr name="ctvText_origin_color" format="color"/>
        <attr name="ctvText_change_color" format="color"/>
        <attr name="ctvProgress" format="float"/>
        <attr name="ctvDirection" format="integer"/>
    </declare-styleable>
</resources>